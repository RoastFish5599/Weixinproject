/**index.wxss**/
.weui-cell {
  display:flex;
  flex-direction: column;
  align-content: center;
  width:94%;
  margin-top:-80px;
}

.weui-input {
  width:100%;
  background-color: #e0e0e0;
  border:1px solid #ccc;
  border-radius: 10px;
}

.weui-pic {
  width:100%;
  margin-top:10px;
}

.swiper {
  height:400rpx;
  width:100%;
}

.swiper img{
  height:50%;
  width:100%;
}

.video {
  width:100%;
  height:100%;
}

.sider {
  width:100%;
  height:20px;
}

.weui-list {
  margin-top:20rpx;
}

.list-box {
  display:flex;
  flex-direction: column;
  margin:20rpx 0;
}

.list-box .list-id {
  display:flex;
  align-items:center;
  margin-left:20rpx;
}

.list-box .list-id .list-msg {
  margin-left:30rpx;
}

.list-box .list-id .list-msg .music-name {
  font-size:32rpx;
}

.list-box .list-id .list-msg .music-author {
  font-size:24rpx;
  width:100%;
  margin-top:5rpx;
}
.list-box .list-id .list-msg .music-author image {
  float:left;
  margin-right:10rpx;
}

.list-box .list-id .list-msg .music-author view {
  float:left;
}
